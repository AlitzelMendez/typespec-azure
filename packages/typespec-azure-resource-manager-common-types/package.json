{
  "name": "@azure-tools/typespec-azure-resource-manager-common-types",
  "version": "0.55.0",
  "description": "Package for generating typespec azure resource manager common types",
  "homepage": "https://azure.github.io/typespec-azure",
  "readme": "https://github.com/Azure/typespec-azure/blob/main/README.md",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Azure/typespec-azure.git"
  },
  "bugs": {
    "url": "https://github.com/Azure/typespec-azure/issues"
  },
  "keywords": [
    "typespec",
    "common-types"
  ],
  "main": "dist/src/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p .",
    "watch": "tsc -p . --watch",
